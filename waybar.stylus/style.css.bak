* {
    border: none;
    border-radius: 10px;
    font-family: "Symbols Nerd Font";
    font-size: 15px;
    min-height: 15px;
    margin-top: 1px;
    margin-left: 1px;
    margin-right: 1px;
    /* margin-left: 8px; */
    margin-bottom: 0px;
    /* opacity: 0.95; */
}


#window, #disk, #workspaces, #network, #pulseaudio, #battery, #backlight, #clock, #memory, #cpu, #tray, #custom-launcher, #idle_inhibitor, #custom-wallpaper, #custom-updates, #custom-media, #mpd{
    margin-left: 10px;
    padding-left: 10px;
    /* margin-right: 10px; */
    padding-right: 10px;
}
window#waybar {
    background: transparent;
}

window#waybar.hidden {
    opacity: 0.2;
}


#mpd {
    background: #66cc99;
    color: #2a5c45;
}

#mpd.disconnected {
    background: #f53c3c;
}

#mpd.stopped {
    background: #90b1b1;
}

#mpd.paused {
    background: #51a37a;
}

#workspaces {
    margin-left: 0px;
    /* font-size: 20px; */
    background: #161320;
    /* margin: 0 4px; */
}

#window {
    background: #161320;
}

#workspaces button {
    color: #B5E8E0;
    background: transparent;
    border-radius: 2px;
    border-top: 1px solid #161320;
    border-bottom: 1px solid #161320;
}

#workspaces button.occupied {
    color: #F28FAD;
    background: transparent;
    font-size: 4px;
}

#workspaces button.active {
    color: #ABE9B3;
    border-top: 1px solid #ABE9B3;
    border-bottom: 1px solid #ABE9B3;
}

#workspaces button:hover {
    box-shadow: inherit;
    text-shadow: inherit;
    color: #FAE3B0;
    border-color: #E8A2AF;
    color: #E8A2AF;
}

#workspaces button.active:hover {
    color: #E8A2AF;
}

#network {
    color: #161320;
    background: #bd93f9;
}

#pulseaudio {
    color: #1A1826;
    background: #FAE3B0;
}

#battery {
    color: #161320;
    background: #B5E8E0;
}

#battery.charging,
#battery.plugged {
    color: #161320;
    background: #B5E8E0;
}

#battery.critical:not(.charging) {
    background: #B5E8E0;
    color: #161320;
    animation-name: blink;
    animation-duration: 0.5s;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
    animation-direction: alternate;
}

@keyframes blink {
    to {
        background: #BF616A;
        color: #B5E8E0;
    }
}

#backlight {
    color: #161320;
    background: #F8BD96;
}

#clock {
    color: #161320;
    background: rgba(66,150,99, 0.95);
    /* background: #ABE9B3; */
    /*background: #1A1826;*/
}

#memory {
    color: #161320;
    background: #DDB6F2;
}

#disk {
    color: #161320;
    background: #DDB6F2;
}

#cpu {
    color: #161320;
    background: #96CDFB;
}

#tray {
    color: #B5E8E0;
    background: #161320;
}

#custom-launcher {
    padding-right: 5px;
    margin-left: 0px;
    color: #89DCEB;
    background: #161320;
}

#idle_inhibitor {
    margin-right: 8px;
    /* padding-right: 5px; */
    color: #161320;
    background: #F28FAD;
}

#custom-wallpaper {
    color: #161320;
    background: #C9CBFF;
}

#custom-updates {
    color: #161320;
    background: #E8A2AF;
}

#custom-media {
    color: #161320;
    background: #F2CDCD;
}


